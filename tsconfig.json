{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "removeComments": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "types": [
      "node",
      "jasmine"
    ],
    "baseUrl": ".",
    "paths": {
      "plotly.js": [
        "node_modules/plotly.js/dist/plotly.js"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
